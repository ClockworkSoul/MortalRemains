#include <mudlib.h>
#include <sewers.h>

inherit OBJECT;
   void create(){
   seteuid(getuid());
   set("author", "cattt");
   set("id", ({ "gem","sapphire" }) );
   set("short", "a precious gem");
   set("long", @Gem
A precious gem. The sapphire is the prettiest color blue you have ever 
seen.  The shape is, however, unusual.  The gem has five points in a 
star layout. It looks quite valuable if for nothing else but its
uniqueness.
Gem
  );
   set("value", 1000);
 }
